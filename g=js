			method: "GET",
			url: "/cgi-bin/OnlineKatalog/Vorteilsnummer.pl",
			data: {
				Rabatt: "1",
				CheckOut: CC
			},
			success: function(data) {
				jQuery("#ergebnis2").text(data);
				var obj = JSON.parse(data);
				jQuery("#Nettobestellwert").html('<b>' + FormatPrice(obj.WKWAEHRUNG, obj.WKWARENWERT) + '</b>');
				if (obj.WKRABATTPROZ > 0) {
