				}
		},
		_toggle: function(a, b, d, h, f) {
			var g = this
			  , e = g.options;
			g.toShow = a;
			g.toHide = b;
			g.data = d;
			var j = function() {
				if (g)
					return g._completed.apply(g, arguments)
			}
			;
