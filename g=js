;(function(c) {
	c.effects.fold = function(a) {
		return this.queue(function() {
			var b = c(this)
			  , j = ["position", "top", "bottom", "left", "right"]
			  , d = c.effects.setMode(b, a.options.mode || "hide")
			  , g = a.options.size || 15
