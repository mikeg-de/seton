					} else {
						jQuery("#Rabatt-Prozent").html('Rabatt aus Vorteilsnummer  ' + obj.WKVORTEILSNUMMER + ' (' + obj.WKRABATTPROZ + '%)');
					}
					jQuery("#Rabatt-Wert").html('-&nbsp;' + FormatPrice(obj.WKWAEHRUNG, obj.WKRABATT).replace(/ /g, ""));
					jQuery("#Rabatt").show();
				} else {
					jQuery("#Rabatt").hide();
				}
				jQuery("#Servicepauschalen").html(FormatPrice(obj.WKWAEHRUNG, obj.WKSERVICEWERT));
				jQuery("#Gesamtnettowert").html('<b>' + FormatPrice(obj.WKWAEHRUNG, obj.WKBETRAGNETTO) + '</b>');
				jQuery("#Frachtkosten").html(FormatPrice(obj.WKWAEHRUNG, obj.WKFRACHTKOSTEN));
				jQuery("#GesamtMwSt").html(FormatPrice(obj.WKWAEHRUNG, obj.WKMWST));
				jQuery("#Gesamtsumme").html('<b>' + FormatPrice(obj.WKWAEHRUNG, obj.WKBETRAG) + '</b>');
				jQuery("#WKPosHead").text(obj.WKPOSITIONEN);
				jQuery("#WKBetragHead").html(FormatPrice(obj.WKWAEHRUNG, obj.WKWARENWERT));
				if (parseFloat(obj.WKDISCOUNT) > 0) {
					if (obj.WKWAEHRUNG == "EUR") {
						jQuery(".sparinfo-gesamt").html("Sie sparen bei dieser Bestellung " + FormatPrice(obj.WKWAEHRUNG, obj.WKDISCOUNT) + " netto");
					} else {
						jQuery(".sparinfo-gesamt").html("Sie sparen bei dieser Bestellung " + FormatPrice(obj.WKWAEHRUNG, obj.WKDISCOUNT));
					}
				} else {
					jQuery(".sparinfo-gesamt").html("");
				}
			},
			cache: false
		});
	}
	function GeschenkeAusgeben() {
		jQuery.get("/cgi-bin/OnlineKatalog/Vorteilsnummer.pl", "Geschenke=1", function(data) {
			jQuery("#ergebnis3").text(data);
			var obj = JSON.parse(data);
			if (!(obj.KEINGESCHENK)) {
				htmlgeschenke = '<tr id="geschenke"><td colspan="2"><h3>Unser Dankesch&ouml;n-Geschenk f&uuml;r Ihre Bestellung aus Vorteilsnummer ' + obj.VORTEILSNUMMER + ':</h3></td></tr>';
				for (i = 1; i <= 4; i++) {
					for (j = 1; j <= 4; j++) {
						objARTNR = eval("obj.ARTNR" + i + j);
						if (objARTNR) {
							objNAME = eval("obj.NAME" + i + j);
							objCHECKED = eval("obj.CHECKED" + i + j);
							if (objCHECKED == 'j') {
								checked = 'checked';
								trCheckedClass = 'Dankeschoen-Geschenk';
							} else {
								checked = '';
								trCheckedClass = '';
							}
							objAKTIV = eval("obj.AKTIV" + i + j);
							if (objAKTIV == 'j') {
								htmlgeschenke = htmlgeschenke + '<tr class="vortn ' + trCheckedClass + '"><td class="no-border Radio-Button" width="5%"><input type="radio" name="Vorteilsnummer_Geschenk" value="' + objARTNR + '" ' + checked + '></td><td class="no-border" width="95%">' + objNAME + '</td></tr>';
							} else {
								objABWERT = eval("obj.ABWERT" + i);
								objWAEHRUNG = eval("obj.WAEHRUNG" + i);
								if (j > 0) {
									htmlgeschenke = htmlgeschenke + '<tr class="inactive no-border vortn"><td class="no-border Radio-Button" width="5%">ab&nbsp;' + FormatPrice(objWAEHRUNG, objABWERT) + '</td><td class="no-border" width="95%">' + objNAME + '</td></tr>';
								} else {
									htmlgeschenke = htmlgeschenke + '<tr class="inactive no-border vortn"><td class="no-border Radio-Button" width="5%"></td><td class="no-border" width="95%">' + objNAME + '</td></tr>';
								}
							}
						}
					}
				}
			} else {
				htmlgeschenke = '<tr id="geschenke" class="no-border"><td colspan="2" class="no-border" style="height:0; font-size:0; line-height:0 !; overflow:hidden; padding:0 !important;"></td></tr>';
			}
			jQuery(".vortn").remove();
			jQuery("#geschenke").replaceWith(htmlgeschenke);
		});
	}
	TextVorteilsnummer = GET('vn');
	if (TextVorteilsnummer != 'undefined' && TextVorteilsnummer.length > 0) {
		InitVorteilsnummer(TextVorteilsnummer);
	}
	function DatenCheck() {
		jQuery.get("/cgi-bin/OnlineKatalog/Vorteilsnummer.pl", "Check=1", function(data) {
			if (data.length > 1) {
				var obj = JSON.parse(data);
				var htmlhinweis = '';
				if (!(obj.FEHLERCODE)) {
					htmlrabatt = '';
					htmlgeschenke = '';
					switch (obj.AKTIONSCODE) {
					case 'A0':
						htmlhinweis = '<p class="Hinweis">Die Vorteilsnummer ' + obj.MKEY_NR + ' ist nicht mehr g&uuml;ltig.</p>';
						break;
					case 'A1':
						htmlhinweis = '<p class="Hinweis">Vorteilsnummer ' + obj.MKEY_NR + ':<br>Die eingegebene Vorteilsnummer ist g&uuml;ltig. Weitere Angaben zu dieser Aktion entnehmen Sie bitte Ihrer Auftragsbest&auml;tigung.</p>';
						break;
					case 'A2':
						if (obj.ABWERT > 0) {
							htmlhinweis = '<p class="Hinweis">Rabattaktion Vorteilsnummer ' + obj.MKEY_NR + ':<br>Sie erhalten ' + obj.RABATT + '% Rabatt auf Ihre Bestellung ab ' + FormatPrice(obj.WAEHRUNG, parseFloat(obj.ABWERT).toFixed(2).replace(".", ",")) + '.</p>';
						} else {
							htmlhinweis = '<p class="Hinweis">Rabattaktion Vorteilsnummer ' + obj.MKEY_NR + ':<br>Sie erhalten ' + obj.RABATT + '% Rabatt auf Ihre Bestellung.</p>';
						}
						break;
					case 'A3':
						htmlhinweis = '<p class="Hinweis">Pr&auml;mienaktion Vorteilsnummer ' + obj.MKEY_NR + ' :<br>Sie erhalten ein Dankesch&ouml;n-Geschenk f&uuml;r Ihre Bestellung.</p>';
						break;
					case 'A4':
						htmlhinweis = '<p class="Hinweis">Aktion Rabatt</h3><p>Vorteilsnummer ' + obj.MKEY_NR + ' <br>Sie erhalten ' + obj.RABATT + '% Preisnachlass und ein Dankesch&ouml;geschenk f&uuml;r Ihre Bestellung.</p>';
						break;
					case 'A5':
						htmlhinweis = '<p class="Hinweis">Vorteilsnummer ' + obj.MKEY_NR + ' <br>Sie erhalten eine Packung Haribo Goldb&auml;ren.</p>';
						break;
					case 'A6':
						htmlhinweis = '<p class="Hinweis">Vorteilsnummer ' + obj.MKEY_NR + ':<br>Sie erhalten ' + obj.RABATT + '% Preisnachlass auf bestimmte Artikel.</p>';
						break;
					case 'A7':
						htmlhinweis = '<p class="Hinweis">Vorteilsnummer ' + obj.MKEY_NR + ':<br>Sie erhalten ' + obj.RABATT + '% Preisnachlass auf bestimmte Artikel<br>und einen Gratisartikel.</p>';
						break;
					}
				} else {}
				GeschenkeAusgeben();
				jQuery("#hinweis").html(htmlhinweis);
			}
		});
	}
	DatenCheck();
	GeschenkeAusgeben();
	RabattAusgeben();
	jQuery(document).ready(function() {
		jQuery.ajaxSetup({
			async: false
		});
		function initDirekt() {
			jQuery(".wkdirekt").click(function() {
				var artNr = jQuery.trim(jQuery("#direkt_art").val());
				var anzahl = jQuery.trim(jQuery("#direkt_anz").val());
				var direktGefunden;
				direktGefunden = false;
				if (isNaN(anzahl) || anzahl <= 0)
					anzahl = 1;
				testReg = /^[a-zA-Z]{0,3}-?([0-9]{6}$)/;
				tmp = testReg.exec(artNr);
				if (tmp != null ) {
					if (tmp[1] != null ) {
						var parameter = {
							rumpfNr: tmp[1],
							getAjaxTestRumpfnr: 1
						};
						jQuery.post('/Warenkorb/Direkt', parameter, function(data) {
							if (data > 1) {
								var umleitung = "/D9" + tmp[1].substr(0, 4) + "000/found.html?Rumpfnr=" + tmp[1];
								window.location.href = umleitung;
								direktGefunden = true;
								return ( false) ;
							}
						});
					}
				}
				testReg = /^[a-zA-Z]{0,3}-?([0-9]{6,10}$)/;
				tmp = testReg.exec(artNr);
				if (tmp != null ) {
					if (tmp[1] != null ) {
						var parameter = {
							artNr: tmp[1],
							getAjaxTestArtikelnr: 1
						};
						jQuery.post('/Warenkorb/Direkt', parameter, function(data) {
							if (data > 1) {
								direktGefunden = true;
								jQuery.post('/Warenkorb/Add', {
									artnr: data,
									Anzahl: anzahl,
									noReturnHTML: true
								}, function(data) {});
								jQuery("#direkt_art").val('');
								jQuery("#direkt_anz").val('');
								reloadCartItems();
								return ( false) ;
							}
						});
					}
				}
				if (!direktGefunden) {
					if (artNr != "6 oder 10 stellig") {
						artNr = artNr.replace(/[^0-9]/g, "");
					}
					artNr = artNr.trim();
					jQuery("#direkt_art").val(artNr);
					if (artNr != "6 oder 10 stellig" && artNr != "") {
						window.location.href = "/Warenkorb/Direkt?artnr=" + artNr + "&anzahl=" + anzahl;
					}
				}
				return ( false) ;
			});
		}
		function init() {
			jQuery('td.Anzahl').removeClass('Anzahl').addClass('Anzahl-JS');
			jQuery(".dateiUpload").colorbox({
				width: "650px",
				height: "70%",
				iframe: true,
				onClosed: function() {
					reloadCartItems();
				}
			});
			jQuery(".direktMitbestellenLink").colorbox({
				width: "80%",
				height: "80%",
				iframe: true,
				onClosed: function() {
					window.setTimeout(function() {
						reloadCartItems();
					});
				}
			});
			jQuery(".editArtikel").each(function() {
				var wkid = jQuery(this).attr("id").substr(8, 99999);
				setChangeUrl(wkid);
			});
			jQuery('input[id^="spin_"]').change(function() {
				var wkid = jQuery(this).attr("id").substr(5, 99999);
				setChangeUrl(wkid);
			});
			jQuery.spin.imageBasePath = '/images/Icons/';
			jQuery('input[id^="spin_"]').spin({
				timeInterval: 250,
				min: 1,
				lock: false,
				changed: function(n) {
					var wkid = jQuery(this).attr("id").substr(5, 99999);
					changeAnzahl(wkid, n, 1);
				}
			});
			jQuery('a[id^="wkdelete_"]').click(function() {
				jQuery('#Warenkorb-Rahmen').showLoading();
				var wkid = jQuery(this).attr("id").substr(9, 99999);
				jQuery.post('/Warenkorb/delete/' + wkid, {
					ajax: 1
				}, function(data) {});
				jQuery("#warenkorbItem_" + wkid).remove();
				reloadCartItems();
				jQuery('#Warenkorb-Rahmen').hideLoading();
				return ( false) ;
			});
			jQuery('a[id^="wkdelete_"]').confirm({
				timeout: 0,
				msg: "Sicher?<br>",
				dialogShow: 'fadeIn',
				dialogSpeed: 'slow',
				buttons: {
					ok: 'Ja',
					cancel: 'Nein',
					cls: 'button-confirm',
					wrapper: '<button></button>',
					separator: '  '
				}
			});
			jQuery("#direkt_art").focus(function() {
				if (jQuery(this).val() == "6 oder 10 stellig") {
					jQuery(this).val("");
					jQuery(this).removeClass("DirektbestellungInputGrey");
				}
			});
			jQuery("#direkt_anz").focus(function() {
				if (jQuery(this).val() == "Anzahl") {
					jQuery(this).val("");
					jQuery(this).removeClass("DirektbestellungInputGrey");
				}
			});
			jQuery('.updateLink').hide();
			changeAnzahl("init", 0);
		}
		init();
		initDirekt();
		function updateVorteilsnummern() {
			DatenCheck();
			GeschenkeAusgeben();
			GeschenkeWahl();
			RabattAusgeben();
		}
		function reloadCartItems() {
			jQuery.post('/Warenkorb', {
				getAjaxCartItems: 1
			}, function(data) {
				if (data == "empty" || jQuery('form[name="Warenkorb"]').length <= 0) {
					jQuery(window.location).attr('href', '/Warenkorb');
				} else {
					updateVorteilsnummern();
					jQuery(".warenkorbItem").remove();
					jQuery(data).insertAfter('form[name="Warenkorb"]');
				}
			}, 'html');
			init();
		}
		function setChangeUrl(wkid) {
			var anzahl = jQuery("#spin_" + wkid).val();
			var artNr = jQuery("#aendern_" + wkid).attr("name");
			var attrhref = jQuery("#aendern_" + wkid).attr("href");
			if (typeof attrhref != 'undefined' && attrhref != false) {
				if (jQuery("#aendern_" + wkid).attr("href").substring(0, 3) == "/D9") {
					jQuery("#aendern_" + wkid).attr("href", "/D9" + String(artNr).substr(0, 4) + "000/Show.html?aendern=1&Artikelnr=" + artNr + "&wkid=" + wkid);
				}
			}
		}
		function changeAnzahl(wkid, n, fromSpin) {
			var timer = 0;
			if (wkid == "init") {
				jQuery('input[id^="spin_"]').keyup(function() {
					var delay = (function() {
						return function(callback, ms) {
							clearTimeout(timer);
							timer = setTimeout(callback, ms);
						}
						;
					})();
					var wkid = jQuery(this).attr("id").substr(5, 99999);
					var anzahl = jQuery(this).val();
					if (anzahl < 1)
						anzahl = 1;
					delay(function() {
						changeAnzahl(wkid, anzahl, 0);
					}, 2000);
				});
			} else {
				updatePrices(wkid, n, fromSpin, true);
				updateVorteilsnummern();
			}
		}
		function updatePrices(wkid, n, fromSpin, recursion) {
			var parameter = {
				wkupdate: {},
				wkanzahl: {},
				fromSpin: fromSpin,
				ajax: 1
			};
			parameter.wkupdate[wkid] = wkid;
			parameter.wkanzahl[wkid] = n;
			var alterPreis = jQuery("#preis_" + wkid).html();
			jQuery.post('/Warenkorb', parameter, function(data) {
				displayPrices(wkid, data['items'][wkid]['artikel']['price']);
				rumpfNr = data['items'][wkid]['artnr'].substr(0, 6);
				if (recursion && alterPreis != data['items'][wkid]['artikel']['price']['preis']) {
					jQuery('.rumpfnr_' + rumpfNr).each(function(i) {
						var wkidChange = jQuery(this).attr("id").substr(5, 99999);
						if (wkidChange != wkid)
							updatePrices(wkidChange, jQuery("#spin_" + wkidChange).val(), 0, false);
					});
				}
			}, 'json');
		}
		function displayPrices(wkid, preise) {
			jQuery("#preis_" + wkid).html(preise['preis']);
			jQuery("#preisreduziert_" + wkid).html(preise['preisreduziert']);
			if (preise['gesamtpreisdiscount'].length > 0 && preise['gesamtpreisdiscount'].substr(0, 4) != "0,00") {
				jQuery("#gesamtpreis_" + wkid).html(preise['gesamtpreis'] + " <div style=\"color:red;\">Sie sparen " + preise['gesamtpreisdiscount'] + "</div>");
			} else {
				jQuery("#gesamtpreis_" + wkid).html(preise['gesamtpreis']);
			}
			jQuery("#spin_" + wkid).val(preise['anzahl']);
		}
		jQuery('#formWkdirekt').submit(function() {
			return ( false) ;
		});
	});
}
;jQuery.getFeed = function(options) {
	options = jQuery.extend({
		url: null ,
		data: null ,
		success: null
	}, options);
	if (options.url) {
		jQuery.ajax({
			type: 'GET',
			url: options.url,
			data: options.data,
			dataType: 'xml',
			success: function(xml) {
				var feed = new JFeed(xml);
				if (jQuery.isFunction(options.success))
					options.success(feed);
			}
		});
	}
}
;
function JFeed(xml) {
	if (xml)
		this.parse(xml);
}
;JFeed.prototype = {
	type: '',
	version: '',
	title: '',
	link: '',
	description: '',
	parse: function(xml) {
		if (jQuery('channel', xml).length == 1) {
			this.type = 'rss';
			var feedClass = new JRss(xml);
		} else if (jQuery('feed', xml).length == 1) {
			this.type = 'atom';
			var feedClass = new JAtom(xml);
		}
		if (feedClass)
			jQuery.extend(this, feedClass);
	}
};
function JFeedItem() {}
;JFeedItem.prototype = {
	title: '',
	link: '',
	description: '',
	updated: '',
	id: ''
};
function JAtom(xml) {
	this._parse(xml);
}
;JAtom.prototype = {
	_parse: function(xml) {
		var channel = jQuery('feed', xml).eq(0);
		this.version = '1.0';
		this.title = jQuery(channel).find('title:first').text();
		this.link = jQuery(channel).find('link:first').attr('href');
		this.description = jQuery(channel).find('subtitle:first').text();
		this.language = jQuery(channel).attr('xml:lang');
		this.updated = jQuery(channel).find('updated:first').text();
		this.items = [];
		var feed = this;
		jQuery('entry', xml).each(function() {
			var item = new JFeedItem();
			item.title = jQuery(this).find('title').eq(0).text();
			item.link = jQuery(this).find('link').eq(0).attr('href');
			item.description = jQuery(this).find('content').eq(0).text();
			item.updated = jQuery(this).find('updated').eq(0).text();
			item.id = jQuery(this).find('id').eq(0).text();
			feed.items.push(item);
		});
	}
};
function JRss(xml) {
	this._parse(xml);
}
;JRss.prototype = {
	_parse: function(xml) {
		if (jQuery('rss', xml).length == 0)
			this.version = '1.0';
		else
			this.version = jQuery('rss', xml).eq(0).attr('version');
		var channel = jQuery('channel', xml).eq(0);
		this.title = jQuery(channel).find('title:first').text();
		this.link = jQuery(channel).find('link:first').text();
		this.description = jQuery(channel).find('description:first').text();
		this.language = jQuery(channel).find('language:first').text();
		this.updated = jQuery(channel).find('lastBuildDate:first').text();
		this.items = [];
		var feed = this;
		jQuery('item', xml).each(function() {
			var item = new JFeedItem();
			item.title = jQuery(this).find('title').eq(0).text();
			item.link = jQuery(this).find('link').eq(0).text();
			item.description = jQuery(this).find('description').eq(0).text();
			item.updated = jQuery(this).find('pubDate').eq(0).text();
			item.id = jQuery(this).find('guid').eq(0).text();
			feed.items.push(item);
		});
	}
};
;function BlogTeaserFormatDate(Datum) {
	Tag = Datum.getDate();
	if (Tag < 10)
		Tag = "0" + Tag;
	Monat = Datum.getMonth() + 1;
	if (Monat < 10)
		Monat = "0" + Monat;
	Jahr = Datum.getYear();
	if (Jahr < 1000)
		Jahr = Jahr + 1900;
	return Tag + "." + Monat + ".";
}
function BlogTeaserFormatTitle(Title, maxLength) {
	Startlaenge = Title.length;
	while (Title.length > maxLength) {
		IndexOfBindestrich = Title.lastIndexOf("-")
		IndexOfLeerzeichen = Title.lastIndexOf(" ")
		if (IndexOfBindestrich > IndexOfLeerzeichen) {
			Ende = IndexOfBindestrich;
		} else {
			Ende = IndexOfLeerzeichen;
		}
		Title = Title.substr(0, Ende);
	}
	if (Startlaenge > Title.length) {
		Title = Title + "...";
	}
	return Title;
}
if (jQuery("#BlogTeaser-result").length > 0) {
	domain = location.hostname;
	Land = domain.substr(domain.length - 2, 2);
	maxlength = 24;
	maxanz = 5;
	jQuery.getFeed({
		url: '/blog-rss/setonblog-rss2-' + Land + '.xml',
		success: function(feed) {
			var html = '<ul>';
			for (var i = 0; i < feed.items.length && i < maxanz; i++) {
				var item = feed.items[i];
				html += '<li><a href="'
				+ item.link
				+ '" target="_blank" alt="' + item.title + '" title="' + item.title + '">'
				+ '<span class="updated">'
				+ BlogTeaserFormatDate(new Date(item.updated))
				+ '</span>'
				+ '&nbsp;'
				+ BlogTeaserFormatTitle(item.title, maxlength)
				+ '</a></li>';
			}
			html += '</ul>';
			jQuery('#BlogTeaser-result').html(html);
		}
	});
}
;ReportFormValidationGMT = true;
function TestAllValid(TestEmpty, EnableFirst) {
	var areAllValid;
	var Ergebnis = true;
	ReportFormValidationGMT = false;
	jQuery('input[type="text"][id^="kontakt_"], input[type="textarea"][id^="kontakt_"]').each(function() {
		if (jQuery(this).prop('id') != "query" && jQuery(this).prop('id').substr(0, 6) != "anzahl") {
			if (TestEmpty || jQuery(this).val().length > 0) {
				if (jQuery(this).prop('id').length > 0) {
					typ = "";
					try {
						obj = "LV_" + jQuery(this).prop('id');
						typ = typeof (eval(obj));
					} catch (err) {}
					if (typ != "" && typ != "undefined") {
						if (EnableFirst) {
							jQuery(this).removeAttr("disabled");
						}
						areAllValid = LiveValidation.massValidate([eval(obj)]);
						if (EnableFirst) {
							jQuery(this).prop("disabled", true);
						}
						if (!areAllValid) {
							Ergebnis = false;
						}
					}
				}
			}
		}
	});
	if (EnableFirst) {
		jQuery('.LV_valid').html("");
	}
	ReportFormValidationGMT = true;
	return Ergebnis;
}
function SetFragenAnregungenMessages() {
	Hinweistext['kontakt_Bemerkungen'] = "Bitte ergänzen Sie dieses Feld.";
	Hinweistext['kontakt_Kundennummer'] = "Geben Sie bitte Ihre Kundennummer ein, wenn Sie bereits SETON Kunde sind.";
	Hinweistext['kontakt_Firma'] = "Geben Sie bitte Ihre offizielle Firmierung ein.";
	Hinweistext['kontakt_Vorname'] = "Geben Sie bitte Ihren Vornamen ein.";
	Hinweistext['kontakt_Nachname'] = "Geben Sie bitte Ihren Familiennamen ein.";
	Hinweistext['kontakt_Abteilung_Position'] = "Geben Sie bitte Ihre Abteilung bzw. Position ein.";
	Hinweistext['kontakt_Strasse'] = "Geben Sie bitte Ihre Straße und Hausnummer ein.";
	Hinweistext['kontakt_PLZ'] = "Geben Sie bitte Ihre Postleitzahl an.";
	Hinweistext['kontakt_Ort'] = "Geben Sie bitte Ihren Ort ein.";
	Hinweistext['kontakt_Telefon'] = "Geben Sie bitte Ihre Telefonnummer ein.";
	Hinweistext['kontakt_Telefax'] = "Geben Sie bitte Ihre Faxnummer ein.";
	Hinweistext['kontakt_Email'] = "Geben Sie bitte Ihre E-Mail-Adresse ein, damit wir Ihre Anfrage per Mail beantworten können.";
	Hinweistext['kontakt_Website'] = "Geben Sie bitte die Internetadresse Ihrer Unternehmeswebsite an.";
	LV_kontakt_Bemerkungen = new LiveValidation('kontakt_Bemerkungen',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Bemerkungen",
		wait: 500
	});
	LV_kontakt_Bemerkungen.add(Validate.Presence, {
		failureMessage: "Bitte ergänzen Sie dieses Feld."
	});
	LV_kontakt_Kundennummer = new LiveValidation('kontakt_Kundennummer',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Kundennummer",
		wait: 500
	});
	LV_kontakt_Firma = new LiveValidation('kontakt_Firma',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Firma",
		wait: 500
	});
	LV_kontakt_Vorname = new LiveValidation('kontakt_Vorname',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Vorname",
		wait: 500
	});
	LV_kontakt_Nachname = new LiveValidation('kontakt_Nachname',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Nachname",
		wait: 500
	});
	LV_kontakt_Nachname.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Der Name muss ausgefüllt sein."
	});
	LV_kontakt_Abteilung_Position = new LiveValidation('kontakt_Abteilung_Position',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Abteilung_Position",
		wait: 500
	});
	LV_kontakt_Strasse = new LiveValidation('kontakt_Strasse',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Strasse",
		wait: 500
	});
	SetKontaktPLZMessages(jQuery(':input[name="kontakt_Land"]').val());
	LV_kontakt_Ort = new LiveValidation('kontakt_Ort',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Ort",
		wait: 500
	});
	LV_kontakt_Telefon = new LiveValidation('kontakt_Telefon',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Telefon",
		wait: 500
	});
	LV_kontakt_Telefon.add(Validate.Format, {
		pattern: /^[\d\+\-\(\)/\s]*$/i,
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Folgende Zeichen sind neben Ziffern zulässige Eingaben: ()-+/"
	});
	LV_kontakt_Telefax = new LiveValidation('kontakt_Telefax',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Telefax",
		wait: 500
	});
	LV_kontakt_Telefax.add(Validate.Presence, {
		failureMessage: "Überprüfen Sie bitte Ihre Eingabe: Ihre Faxnummer muss ausgefüllt sein."
	});
	LV_kontakt_Telefax.add(Validate.Format, {
		pattern: /^[\d\+\-\(\)/\s]*$/i,
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Folgende Zeichen sind neben Ziffern zulässige Eingaben: ()-+/"
	});
	if (jQuery(':checkbox[name="newsletter"]').is(':checked')) {
		LV_kontakt_Email = new LiveValidation('kontakt_Email',{
			validMessage: 'OK. Um Ihre Daten vor Missbrauch zu schützen, erhalten Sie einen Bestätigungslink an die angegebene E-Mail-Adresse. Erst nach Bestätigung dieses Links wird Ihre Newsletteranmeldung gültig.',
			insertAfterWhatNode: "Hinweistext_kontakt_Email",
			wait: 500
		});
		LV_kontakt_Email.add(Validate.Presence, {
			failureMessage: "Sie haben einen Haken für unseren Informationsservice per Newsletter gesetzt. Bitte tragen Sie Ihre E-Mail-Adresse ein."
		});
	} else {
		LV_kontakt_Email = new LiveValidation('kontakt_Email',{
			validMessage: "OK",
			insertAfterWhatNode: "Hinweistext_kontakt_Email",
			wait: 500
		});
		LV_kontakt_Email.add(Validate.Presence, {
			failureMessage: "Geben Sie bitte Ihre E-Mail-Adresse ein, damit wir Ihre Anfrage per Mail beantworten können."
		});
	}
	LV_kontakt_Email.add(Validate.Email, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Die E-Mail-Adresse muss ein @ sowie ein Länderkürzel beinhalten, z.B. .de, .at, .ch, .com, ..."
	});
	jQuery(':checkbox[name="newsletter"]').change(function() {
		LV_kontakt_Email.destroy();
		if (jQuery(':checkbox[name="newsletter"]').is(':checked')) {
			LV_kontakt_Email = new LiveValidation('kontakt_Email',{
				validMessage: 'OK. Um Ihre Daten vor Missbrauch zu schützen, erhalten Sie einen Bestätigungslink an die angegebene E-Mail-Adresse. Erst nach Bestätigung dieses Links wird Ihre Newsletteranmeldung gültig.',
				insertAfterWhatNode: "Hinweistext_kontakt_Email",
				wait: 500
			});
			LV_kontakt_Email.add(Validate.Presence, {
				failureMessage: "Sie haben einen Haken für unseren Informationsservice per Newsletter gesetzt. Bitte tragen Sie Ihre E-Mail-Adresse ein."
			});
		} else {
			LV_kontakt_Email = new LiveValidation('kontakt_Email',{
				validMessage: "OK",
				insertAfterWhatNode: "Hinweistext_kontakt_Email",
				wait: 500
			});
			LV_kontakt_Email.add(Validate.Presence, {
				failureMessage: "Geben Sie bitte Ihre E-Mail-Adresse ein, damit wir Ihre Anfrage per Mail beantworten können."
			});
		}
		LV_kontakt_Email.add(Validate.Email, {
			failureMessage: "Überprüfen Sie bitte Ihre Eingabe: Die E-Mail-Adresse muss ein @ sowie ein Länderkürzel beinhalten, z.B. .de, .at, .ch, .com, ..."
		});
		jQuery(':input[id="kontakt_Email"]').focus();
		jQuery('.Hinweistext').html('');
		jQuery(':checkbox[name="newsletter"]').focus();
		LiveValidation.massValidate(LV_kontakt_Email);
	});
	jQuery(':input[name="Land"]').change(function() {
		LV_kontakt_PLZ.destroy();
		SetKontaktPLZMessages(jQuery(this).val());
		jQuery(':input[id="kontakt_PLZ"]').focus();
		jQuery('.Hinweistext').html('');
		jQuery(':input[name="Land"]').focus();
		LiveValidation.massValidate(LV_kontakt_PLZ);
	});
}
