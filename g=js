	trs.type = 'text/javascript';
	trs.async = true;
	trs.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//trs.webprospector.de/trs.js';
	var trs_t = document.getElementsByTagName('script')[0];
	trs_t.parentNode.insertBefore(trs, trs_t);
