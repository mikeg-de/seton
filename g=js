		jQuery(".Eingabefeld").each(function() {
			var wert = jQuery(this).val();
			jQuery(this).focusin(function() {
				if (jQuery(this).val() == wert) {
					jQuery(this).val("");
				}
			}).focusout(function() {
				if (jQuery.trim(jQuery(this).val()) === "") {
					jQuery(this).val(wert);
				}
			});
		});
	});
