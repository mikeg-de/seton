		c.fn.extend({
			_focus: c.fn.focus,
			focus: function(a, b) {
				return typeof a === "number" ? this.each(function() {
					var d = this;
					setTimeout(function() {
						c(d).focus();
						b && b.call(d)
					}, a)
				}) : this._focus.apply(this, arguments)
			},
