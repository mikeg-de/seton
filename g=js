						this.active = b ? c([]) : a;
						this._toggle(j, g, e, b, f);
						h.removeClass("ui-state-active ui-corner-top").addClass("ui-state-default ui-corner-all").children(".ui-icon").removeClass(d.icons.headerSelected).addClass(d.icons.header);
