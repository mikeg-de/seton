HTTP_GET_VARS = [];
strGET = document.location.search.substr(1, document.location.search.length);
if (strGET !== '')
{
	gArr = strGET.split('&');
	for (i = 0; i < gArr.length; ++i)
	{
		v = '';
		vArr = gArr[i].split('=');
		if (vArr.length > 1) {
			v = vArr[1];
		}
		HTTP_GET_VARS[unescape(vArr[0])] = unescape(v);
	}
}
function GET(v) {
	if (!HTTP_GET_VARS[v]) {
		return 'undefined';
	}
	return HTTP_GET_VARS[v];
}
if (typeof jQuery != 'undefined') {
	jQuery(document).ready(function($) {
		if (iOS) {
			touchelement = "";
			jQuery(".topnav-notouch").bind("touchstart", function(event) {
				parentid = jQuery(this).parent().attr("id");
				if (touchelement != parentid) {
					touchelement = parentid;
					event.preventDefault();
				} else {
					touchelement = "";
				}
			});
		}
		var filetypes = /\.(zip|exe|pdf|doc*|xls*|ppt*|mp3|mp4)$/i;
		var baseHref = '';
		if (jQuery('base').attr('href') !== undefined)
			baseHref = jQuery('base').attr('href');
		jQuery('a').each(function() {
			var href = jQuery(this).attr('href');
			if (href && (href.match(/^https?\:/i)) && (!href.match(document.domain))) {
				jQuery(this).click(function() {
					var extLink = href.replace(/^https?\:\/\//i, '');
					try {
						_gaq.push(['_trackEvent', 'External', 'Click', extLink]);
					} catch (err) {}
					if (jQuery(this).attr('target') !== undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
						setTimeout(function() {
							location.href = href;
						}, 200);
						return false;
					}
				});
			}
			else if (href && href.match(/^mailto\:/i)) {
				jQuery(this).click(function() {
					var mailLink = href.replace(/^mailto\:/i, '');
					try {
						_gaq.push(['_trackEvent', 'Email', 'Click', mailLink]);
					} catch (err) {}
				});
			}
			else if (href && href.match(filetypes)) {
				jQuery(this).click(function() {
					var extension = (/[.]/.exec(href)) ? /[^.]+$/.exec(href) : undefined;
					var filePath = href;
					try {
						_gaq.push(['_trackEvent', 'Download', 'Click-' + extension, filePath]);
					} catch (err) {}
					if (jQuery(this).attr('target') !== undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
						setTimeout(function() {
							location.href = baseHref + href;
						}, 200);
						return false;
					}
				});
			}
		});
	});
}
jQuery(window).load(function() {
	loadLastSeenProducts();
	if (jQuery('#DTSF').length === 0) {
		if (jQuery('#DMSF').length > 0) {
			jQuery('#Footer_Bereichstext').insertBefore(jQuery('#lastSeenProducts'));
			if (jQuery('#Footer_Bereichstext div.Content-Footer-Bereichstext').length > 0) {
				jQuery('#Footer_Bereichstext div.Content-Footer-Bereichstext').addClass('col9').addClass('right');
			}
		} else {
			jQuery('#Footer_Bereichstext').insertAfter(jQuery('#lastSeenProducts'));
		}
	}
	if (jQuery('#BlogFeed').length > 0) {
		jQuery('#BlogFeed').html(jQuery('#BlogFeedOriginal').html());
		jQuery('#BlogFeedOriginal').remove();
	}
	SetGAFormularTracking();
	jQuery(".Info-Link .Info-Text").removeClass("Info-Text").addClass("Info-Text-JS").hide();
	jQuery(".Info-Link .Info-Text-JS").append("<div class='Button-close'></div>");
	jQuery(".Info-Link").click(function() {
		jQuery(this).children(".Info-Text-JS").toggle();
	});
	jQuery("#Rotating-image").colorbox({
		width: "470px",
		height: "430px",
		fastIframe: false,
		iframe: true
	});
});
function FFclickProduct(query, id, pos, origPos, page, simi, title, pageSize, origPageSize, channel, event, count, price) {
	if (event === null ) {
		event = "click";
	}
	if (count === null ) {
		count = "";
	}
	if (price === null ) {
		price = "";
	}
	sid = "";
	if (Get_Cookie("SETON-ID")) {
		sid = Get_Cookie('SETON-ID');
	}
	jQuery.ajax({
		type: "GET",
		url: "/cgi-bin/FFTracking.pl",
		data: {
			query: query,
			id: id,
			pos: pos,
			origPos: origPos,
			page: page,
			simi: simi,
			sid: sid,
			title: title,
			event: event,
			pageSize: pageSize,
			origPageSize: origPageSize,
			channel: channel,
			count: count,
			price: price
		},
		contentType: "application/x-www-form-urlencoded; charset=UTF-8",
		cache: false,
		async: false,
	});
}
function SetGATracking() {}
function GetReferrerSearchstring() {
	if (document.referrer) {
		Suchstringkombination = [];
		Suchstringkombination[0] = {};
		Suchstringkombination[0].Domain = "seton";
		Suchstringkombination[0].ref = "intern";
		Suchstringkombination[0].paramname = "query";
		Suchstringkombination[0].searchstring = "";
		Suchstringkombination[1] = {};
		Suchstringkombination[1].Domain = "google";
		Suchstringkombination[1].ref = "extern";
		Suchstringkombination[1].paramname = "q";
		Suchstringkombination[1].searchstring = "";
		Suchstringkombination[2] = {};
		Suchstringkombination[2].Domain = "lycos";
		Suchstringkombination[2].ref = "extern";
		Suchstringkombination[2].paramname = "query";
		Suchstringkombination[2].searchstring = "";
		Suchstringkombination[3] = {};
		Suchstringkombination[3].Domain = "bing";
		Suchstringkombination[3].ref = "extern";
		Suchstringkombination[3].paramname = "q";
		Suchstringkombination[3].searchstring = "";
		Suchstringkombination[4] = {};
		Suchstringkombination[4].Domain = "aol";
		Suchstringkombination[4].ref = "extern";
		Suchstringkombination[4].paramname = "q";
		Suchstringkombination[4].searchstring = "";
		Suchstringkombination[5] = {};
		Suchstringkombination[5].Domain = "ask";
		Suchstringkombination[5].ref = "extern";
		Suchstringkombination[5].paramname = "q";
		Suchstringkombination[5].searchstring = "";
		Suchstringkombination[6] = {};
		Suchstringkombination[6].Domain = "babylon";
		Suchstringkombination[6].ref = "extern";
		Suchstringkombination[6].paramname = "q";
		Suchstringkombination[6].searchstring = "";
		Suchstringkombination[7] = {};
		Suchstringkombination[7].Domain = "conduit";
		Suchstringkombination[7].ref = "extern";
		Suchstringkombination[7].paramname = "q";
		Suchstringkombination[7].searchstring = "";
		Suchstringkombination[8] = {};
		Suchstringkombination[8].Domain = "ecosia";
		Suchstringkombination[8].ref = "extern";
		Suchstringkombination[8].paramname = "q";
		Suchstringkombination[8].searchstring = "";
		Suchstringkombination[9] = {};
		Suchstringkombination[9].Domain = "suche.t-online.de";
		Suchstringkombination[9].ref = "extern";
		Suchstringkombination[9].paramname = "q";
		Suchstringkombination[9].searchstring = "";
		Suchstringkombination[10] = {};
		Suchstringkombination[10].Domain = "suche.web.de";
		Suchstringkombination[10].ref = "extern";
		Suchstringkombination[10].paramname = "su";
		Suchstringkombination[10].searchstring = "";
		Suchstringkombination[11] = {};
		Suchstringkombination[11].Domain = "yahoo";
		Suchstringkombination[11].ref = "extern";
		Suchstringkombination[11].paramname = "p";
		Suchstringkombination[11].searchstring = "";
		try {
			var strHref = decodeURIComponent(escape(unescape(document.referrer)));
			for (var i = 0; i < Suchstringkombination.length; i++) {
				if (strHref.indexOf(Suchstringkombination[i].Domain) > -1) {
					if (strHref.indexOf("?") > -1) {
						var strQueryString = strHref.substr(strHref.indexOf("?")).toLowerCase();
						var aQueryString = strQueryString.split("&");
						for (var iParam = 0; iParam < aQueryString.length; iParam++) {
							if (aQueryString[iParam].indexOf(Suchstringkombination[i].paramname.toLowerCase() + "=") > -1) {
								var aParam = aQueryString[iParam].split("=");
								Suchstringkombination[i].searchstring = aParam[1];
								return Suchstringkombination[i];
							}
						}
					}
				}
			}
		} catch (err) {}
	}
	return false;
}
function checkFormAbandonment()
{
	if (window.focussedFormElement) {
		if (window.focussedFormElement.type != "submit" && window.focussedFormElement.type != "button") {
			try {
				_gaq.push(['_trackEvent', 'Formularfelder', window.focussedForm.name, window.focussedFormElement.name]);
			} catch (err) {}
			try {
				_gaq.push(['_trackEvent', 'Formularversand', window.focussedForm.name, 'abgebrochen', 0]);
			} catch (err) {}
		} else {
			try {
				_gaq.push(['_trackEvent', 'Formularversand', window.focussedForm.name, 'gesendet', 1]);
			} catch (err) {}
		}
	}
}
function SetGAFormularTracking() {
	window.focussedFormElement = null ;
	window.focussedForm = null ;
	window.onload = function()
	{
		var n = function() {
			window.focussedFormElement = this.form.focussedElement = this;
			window.focussedForm = this.form.focussedForm = this.form;
		};
		p = function() {
			window.focussedFormElement = this.form.focussedElement = null ;
			window.focussedForm = this.form.focussedForm = null ;
		}
		;
		for (var i = 0, f = document.forms; i < f.length; ++i) {
			f[i].focussedElement = null ;
			for (var j = 0, e = f[i].elements; j < e.length; ++j) {
				e[j].onblur = p;
			}
		}
	}
	;
}
function loadLastSeenProducts() {
	if (window.location.pathname.substr(1, 10) != "Bestellung") {
		jQuery.get('/LetzteGeseheneArtikel/', {
			getAjaxLastSeenProducts: 1
		}, function(data) {
			if (data == "empty") {} else {
				jQuery('#lastSeenProducts').html(data);
			}
		}, 'html');
	}
}
