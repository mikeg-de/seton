					}
				} else
					e.push("ui-button-text-only");
				b.addClass(e.join(" "))
			}
		}
	});
	a.widget("ui.buttonset", {
		options: {
			items: ":button, :submit, :reset, :checkbox, :radio, a, :data(button)"
		},
		_create: function() {
			this.element.addClass("ui-buttonset")
