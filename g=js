		},
		_clickHandler: function(a, b) {
			var d = this.options;
			if (!d.disabled)
				if (a.target) {
					a = c(a.currentTarget || b);
					b = a[0] === this.active[0];
					d.active = d.collapsible && b ? false : this.headers.index(a);
