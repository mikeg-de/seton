function Initial360GradSpriteSpin(BildArray360, Bildbreite360, Bildhoehe360) {
	if (typeof jQuery().spritespin == "undefined") {
		jQuery.getScript("/js/spritespin.js", function(data, textStatus, jqxhr) {
			jQuery.getScript("/js/spritespin.beh-drag.js", function(data, textStatus, jqxhr) {
				jQuery.getScript("/js/spritespin.mod-360.js", function(data, textStatus, jqxhr) {
					jQuery.getScript("/js/spritespin.api.js", function(data, textStatus, jqxhr) {
						Initial360Grad(BildArray360, Bildbreite360, Bildhoehe360);
					});
				});
			});
		});
	} else {
		Initial360Grad(BildArray360, Bildbreite360, Bildhoehe360);
	}
}
function Initial360Grad(BildArray360, Bildbreite360, Bildhoehe360) {
	if (typeof BildArray360 != 'undefined') {
		var spin = jQuery('#spritespin');
		var slide = jQuery('#slider');
		var Anzahl360 = BildArray360.length;
		spin.spritespin({
			source: BildArray360,
			width: Bildbreite360,
			height: Bildhoehe360,
			sense: 1,
			frameTime: 100,
			onLoad: function() {
				slide.slider({
					min: 0,
					max: Anzahl360 - 1,
					slide: function(e, ui) {
						var api = spin.spritespin('api');
						api.stopAnimation();
						api.updateFrame(ui.value);
					}
				});
			},
			onFrame: function(e, data) {
				slide.slider('value', data.frame);
			}
		});
		var api = spin.spritespin("api");
		jQuery('#prev').click(function() {
			api.stopAnimation();
			api.prevFrame();
			if (!jQuery('.icon_pause_alt2').hasClass('arrow_triangle-right_alt2'))
				jQuery('.icon_pause_alt2').toggleClass('arrow_triangle-right_alt2');
		});
		jQuery('#next').click(function() {
			api.stopAnimation();
			api.nextFrame();
			if (!jQuery('.icon_pause_alt2').hasClass('arrow_triangle-right_alt2'))
				jQuery('.icon_pause_alt2').toggleClass('arrow_triangle-right_alt2');
		});
		jQuery('#toggle').click(function() {
			api.toggleAnimation();
			jQuery('.icon_pause_alt2').toggleClass('arrow_triangle-right_alt2');
		});
		jQuery(".spritespin-canvas").bind("touchstart", function(event) {
			api.stopAnimation();
			if (!jQuery('.icon_pause_alt2').hasClass('arrow_triangle-right_alt2'))
				jQuery('.icon_pause_alt2').toggleClass('arrow_triangle-right_alt2');
		});
		jQuery(".spritespin-canvas").bind("mousedown", function(event) {
			api.stopAnimation();
			if (!jQuery('.icon_pause_alt2').hasClass('arrow_triangle-right_alt2'))
				jQuery('.icon_pause_alt2').toggleClass('arrow_triangle-right_alt2');
		});
	}
}
function FormularTrackingGTM(status, ElementId, ElementValue, ElementType) {
	if (ReportFormValidationGMT) {
		AktuelleZeit = new Date();
		ZeitaufSeite = Math.round(AktuelleZeit.getTime() - StartAufSeite) / 1000;
		switch (status) {
		case "focus":
			if (LastValidationGMT != "focus") {
				LastValidationGMT = "focus";
			}
			break;
		case "valid":
			if (LastValidationGMT != "valid") {
				LastValidationGMT = "valid";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': window.location.pathname,
					'eventAction': ElementId,
					'eventLabel': 'Input ok',
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		case "invalid":
			if (LastValidationGMT != "invalid") {
				LastValidationGMT = "invalid";
				if (window.location.pathname == "/Bestellung" && ElementId == "kundenadr_PLZ") {
					dataLayer.push({
						'event': 'clickFormElement',
						'eventCategory': window.location.pathname,
						'eventAction': ElementId,
						'eventLabel': 'Error: ' + ElementValue + ' (' + jQuery('#kundenadr_Land').val() + ')',
						'eventValue': ZeitaufSeite,
						'nonInteractive': 0
					});
				} else if (window.location.pathname == "/Bestellung" && ElementId == "lieferadr_PLZ") {
					dataLayer.push({
						'event': 'clickFormElement',
						'eventCategory': window.location.pathname,
						'eventAction': ElementId,
						'eventLabel': 'Error: ' + ElementValue + ' (' + jQuery('#lieferadr_Land').val() + ')',
						'eventValue': ZeitaufSeite,
						'nonInteractive': 0
					});
				} else if (window.location.pathname == "/Bestellung" && ElementId == "rechnadr_PLZ") {
					dataLayer.push({
						'event': 'clickFormElement',
						'eventCategory': window.location.pathname,
						'eventAction': ElementId,
						'eventLabel': 'Error: ' + ElementValue + ' (' + jQuery('#rechnadr_Land').val() + ')',
						'eventValue': ZeitaufSeite,
						'nonInteractive': 0
					});
				} else {
					dataLayer.push({
						'event': 'clickFormElement',
						'eventCategory': window.location.pathname,
						'eventAction': ElementId,
						'eventLabel': 'Error: ' + ElementValue,
						'eventValue': ZeitaufSeite,
						'nonInteractive': 0
					});
				}
			}
			break;
		case "send":
			if (LastValidationGMT != "send") {
				LastValidationGMT = "send";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': window.location.pathname,
					'eventAction': 'Form submit',
					'eventLabel': 'undefined',
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		case "notsend":
			if (LastValidationGMT != "notsend") {
				LastValidationGMT = "notsend";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': window.location.pathname,
					'eventAction': 'Form submit validation error',
					'eventLabel': 'undefined',
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		}
	} else {}
}
function FormularTrackingGTMHTML5(status, ElementId, ElementValue, ElementType, FormId) {
	if (ReportFormValidationGTMHTML5[FormId] == 1) {
		AktuelleZeit = new Date();
		ZeitaufSeite = Math.round(AktuelleZeit.getTime() - StartAufSeite) / 1000;
		switch (status) {
		case "focus":
			if (LastValidationGTMHTML5 != "focus") {
				LastValidationGTMHTML5 = "focus";
			}
			break;
		case "valid":
			if (LastValidationGTMHTML5 != "valid") {
				LastValidationGTMHTML5 = "valid";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': FormId,
					'eventAction': ElementId,
					'eventLabel': 'Input ok',
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		case "invalid":
			if (LastValidationGTMHTML5 != "invalid") {
				LastValidationGTMHTML5 = "invalid";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': FormId,
					'eventAction': ElementId,
					'eventLabel': 'Error: ' + ElementValue,
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		case "send":
			if (LastValidationGTMHTML5 != "send") {
				LastValidationGTMHTML5 = "send";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': FormId,
					'eventAction': 'Form submit',
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		case "notsend":
			if (LastValidationGTMHTML5 != "notsend") {
				LastValidationGTMHTML5 = "notsend";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': FormId,
					'eventAction': 'Form submit validation error',
					'eventLabel': +ElementValue,
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		case "download":
			if (LastValidationGTMHTML5 != "download") {
				LastValidationGTMHTML5 = "download";
				dataLayer.push({
					'event': 'clickFormElement',
					'eventCategory': FormId,
					'eventAction': 'Form submit validation error',
					'eventLabel': +ElementValue,
					'eventValue': ZeitaufSeite,
					'nonInteractive': 0
				});
			}
			break;
		}
	} else {}
}
function CheckFields(formid) {
	var errorstring = "";
	if (typeof document.getElementById(formid)[0].willValidate !== "undefined") {
		jQuery("form#" + formid + " :input").each(function() {
			if (this.checkValidity() === false) {
				if (errorstring.length > 0) {
					errorstring = errorstring + " - ";
				}
				errorstring = errorstring + jQuery(this).attr("id") + ": " + jQuery(this).val();
			}
		});
	}
	if (errorstring.length > 0) {
		FormularTrackingGTMHTML5("notsend", "", errorstring, "", formid);
	}
}
function ValidateInput(item) {
	switch (item.type) {
	case "email":
		if (item.value.length === 0) {
			item.setCustomValidity('Bitte geben Sie Ihre Email an.');
		} else if (item.value.match(/ /)) {
			item.setCustomValidity('Ihre Email-Adresse enthällt Leerschritte.');
		} else if (!item.value.match(/@/)) {
			item.setCustomValidity('Ihre Email-Adresse enthällt kein @.');
		} else if (!item.value.match(/^[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,4}$/)) {
			item.setCustomValidity('Bitte überprüfen Sie Ihre Email-Adresse.');
		} else {
			item.setCustomValidity('');
		}
		break;
	default:
	}
	LastValidationGTMHTML5 = "";
	if (typeof item.willValidate !== "undefined") {
		if (item.validity.valid) {
			FormularTrackingGTMHTML5("valid", item.id, item.value, item.type, jQuery(item).closest('form').attr('id'));
		} else {
			FormularTrackingGTMHTML5("invalid", item.id, item.value, item.type, jQuery(item).closest('form').attr('id'));
		}
	}
}
function NLAnmeldungAjax(formular) {
	var form = jQuery(formular);
	var data = form.serialize();
	var formid = form.attr('id');
	jQuery.post('/skripte/NewsletterAjax.php', data, function(response) {
		if (jQuery(response).find('status').text() == "file") {
			jQuery("#Fehlermeldung").html("");
			datei = jQuery(response).find('file').text();
			FormularTrackingGTMHTML5("send", "", "", "", formid);
			jQuery("#" + formid).html("Vielen Dank! Sollte der Download nicht automatisch starten, klicken Sie bitte  <a href=\"" + datei + "\" onclick=\"FormularTrackingGTMHTML5(\'download\', \'\',\'" + datei + "\', \'\', \'" + formid + "\')\">hier</a>");
			window.location = datei;
		} else if (jQuery(response).find('status').text() == "vtnr") {
			jQuery("#Fehlermeldung").html("");
			FormularTrackingGTMHTML5("send", "", "", "", formid);
			jQuery("#" + formid).html("<p>Die Vorteilsnummer " + jQuery(response).find('vtnr').text() + " wurde im Warenkorb hinterlegt.</p><p class='Klein'>Gültig bis " + jQuery(response).find('gueltig').text() + " | Nicht kombinierbar</p>");
			SetVorteilsnummerOM(jQuery(response).find('vtnr').text());
		} else if (jQuery(response).find('status').text() == "error") {
			FormularTrackingGTMHTML5("notsend", "", jQuery(response).find('error').text(), "", formid);
			jQuery("#Fehlermeldung").html(jQuery(response).find('error').text());
		} else {}
	});
}
