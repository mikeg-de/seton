			}, b, d, e]) : this._toggleClass(c, a) : f.effects.animateClass.apply(this, [{
				toggle: c
			}, a, b, d])
		},
		switchClass: function(c, a, b, d, e) {
			return f.effects.animateClass.apply(this, [{
				add: a,
				remove: c
			}, b, d, e])
		}
