				d(a).parents().each(function() {
					e |= d(this).css("position") == "fixed";
					return !e
				});
				if (e && d.browser.opera) {
					d.datepicker._pos[0] -= document.documentElement.scrollLeft;
					d.datepicker._pos[1] -= document.documentElement.scrollTop
				}
