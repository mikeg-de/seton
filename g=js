		wait: 500
	});
	LV_kontakt_Nachname.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Der Name muss ausgefüllt sein."
	});
	LV_kontakt_Abteilung_Position = new LiveValidation('kontakt_Abteilung_Position',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Abteilung_Position",
		wait: 500
	});
