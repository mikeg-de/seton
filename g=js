		});
	});
}
function SetSetonSuperKickWinMessages() {
	Hinweistext['kontakt_Firma'] = "Geben Sie bitte Ihre offizielle Firmierung ein.";
	Hinweistext['kontakt_Vorname'] = "Geben Sie bitte Ihren Vornamen ein.";
	Hinweistext['kontakt_Nachname'] = "Geben Sie bitte Ihren Familiennamen ein.";
	Hinweistext['kontakt_Email'] = "Geben Sie bitte Ihre E-Mail-Adresse ein.";
	Hinweistext['kontakt_Spielname'] = "Geben Sie bitte einen Spielernamen ein.";
	Hinweistext['kontakt_Passwort1'] = "Geben Sie bitte ein Passwort ein.";
	Hinweistext['kontakt_Passwort2'] = "Wiederholen Sie bitte das eingegebene Passwort, um Tippfehler auszuschließen.";
	Hinweistext['kontakt_Newsletter'] = "Melden Sie sich bitte zum Spielen für den SETON-Newsletter an.";
	LV_kontakt_Firma = new LiveValidation('kontakt_Firma',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Firma",
		wait: 500
	});
	LV_kontakt_Firma.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Der Firmenname muss ausgefüllt sein."
	});
	LV_kontakt_Vorname = new LiveValidation('kontakt_Vorname',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Vorname",
		wait: 500
	});
	LV_kontakt_Vorname.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Der Vorname muss ausgefüllt sein."
	});
	LV_kontakt_Nachname = new LiveValidation('kontakt_Nachname',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Nachname",
		wait: 500
	});
	LV_kontakt_Nachname.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Der Name muss ausgefüllt sein."
	});
	LV_kontakt_Email = new LiveValidation('kontakt_Email',{
		validMessage: 'OK',
		insertAfterWhatNode: "Hinweistext_kontakt_Email",
		wait: 500
	});
	LV_kontakt_Email.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Die E-Mail-Adresse muss ausgefüllt sein."
	});
	LV_kontakt_Email.add(Validate.Email, {
		failureMessage: "Überprüfen Sie bitte Ihre Eingabe: Die E-Mail-Adresse muss ein @ sowie ein Länderkürzel beinhalten, z.B. .de, .at, .ch, .com, ..."
	});
	LV_kontakt_Spielname = new LiveValidation('kontakt_Spielname',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Spielname",
		wait: 500
	});
	LV_kontakt_Spielname.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Der Spielername muss ausgefüllt sein."
	});
	LV_kontakt_Spielname.add(Validate.Format, {
		pattern: /^[0-9a-z]*$/i,
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Der Spielername muss ausgefüllt sein und darf kein Umlaute enthalten. Erlaubt sind nur Buchstaben und Ziffern."
	});
	LV_kontakt_Passwort1 = new LiveValidation('kontakt_Passwort1',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Passwort1",
		wait: 500
	});
	LV_kontakt_Passwort1.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Das Passwort muss ausgefüllt sein."
	});
	LV_kontakt_Passwort2 = new LiveValidation('kontakt_Passwort2',{
		validMessage: "OK",
		insertAfterWhatNode: "Hinweistext_kontakt_Passwort2",
		wait: 500
	});
	LV_kontakt_Passwort2.add(Validate.Presence, {
		failureMessage: "Überpüfen Sie bitte Ihre Eingabe: Das Passwort muss wiederholt werden, um Tippfehler auszuschließen."
	});
	LV_kontakt_Passwort2.add(Validate.Confirmation, {
		match: 'kontakt_Passwort1',
		failureMessage: "Bitte überprüfen Sie Ihre Eingabe. Die Passwörter stimmen nicht überein."
	});
}
